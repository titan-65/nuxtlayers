{
    "name": "nuxt-layers",
    "version": "0.0.1",
    "description": "CLI tool to discover and install Nuxt Layers",
    "keywords": [
        "nuxt",
        "layers",
        "cli",
        "marketplace"
    ],
    "author": "Vantol Bennett",
    "license": "MIT",
    "bin": {
        "nuxt-layers": "./bin/cli.js"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "bin",
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "lint": "eslint src",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "commander": "^12.0.0",
        "prompts": "^2.4.2",
        "chalk": "^5.3.0",
        "ora": "^8.0.0",
        "tar": "^7.0.0",
        "fs-extra": "^11.0.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.0",
        "@types/prompts": "^2.4.0",
        "@types/tar": "^6.1.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    }
}